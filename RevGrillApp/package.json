{
  "name": "rev-grill-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm install concurrently && npm install yarn",
    "setup": "npm i && npm --prefix ./client i && npm --prefix ./api i",
    "dev": "npm run prebuild && concurrently -n front,back -c green,yellow -t \"HH:mm:ss\" -p \"{name} {time}\" \"yarn --cwd ./client start\" \"yarn --cwd ./api start\"",
    "dev-front": "yarn --cwd ./client start",
    "dev-back": "yarn --prefix ./api start",
    "build": "npm --prefix ./client run build && npm --prefix ./api run build"
  },
