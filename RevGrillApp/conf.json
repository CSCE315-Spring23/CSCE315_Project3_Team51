{
    "plugins": [],
    "recurseDepth": 10,
    "source": {
        "include": [ "./client/src/pages/Customer_Side/", "./client/src/pages/Home/", "./client/src/pages/Log_In/",
                     "./client/src/pages/Manager_Side/", "./client/src/pages/Menu_Side/", "./client/src/pages/Server_Side/",
